{
  "name": "cli-boilerplate",
  "version": "1.0.2",
  "description": "Boilerplate for webpack build of browser application",
  "private": true,
  "repository": "git+ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/webpack-boilerplate",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node5": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "commander": "git+https://github.com/snow01/commander.js.git",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-360fy": "https://private-npm-releases.s3.amazonaws.com/eslint-config-360fy-1.0.1.tgz",
    "eslint-plugin-react": "^4.2.3",
    "graceful-readlink": "^1.0.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.6.0"
  }
}
